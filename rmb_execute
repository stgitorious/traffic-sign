python cmd_line/boltzmann/run_rbm.py --dataset custom --main_dir ./rbm-models --model_name pretrain.RBM --train_dataset ~/Programs/traffic/train_set_reshape.npy --valid_dataset ~/Programs/traffic/val_set_reshape.npy --test_dataset ~/Programs/traffic/test_set_reshape.npy --num_hidden 500 --num_epochs 10 --batch_size 100 --learning_rate 0.0001 --gibbs_sampling_steps 3 --verbose 1 --save_parameters ~/Programs/traffic/result/param --save_reconstructions ~/Programs/traffic/results/recon

